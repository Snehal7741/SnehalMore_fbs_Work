s1 ="Firstbit solutions"
s2 = "Firstbit solutions data science"
len1 = len2 = 0

for _ in s1:
    len1 += 1
for _ in s2:
    len2 += 1

if len1 > len2:
    print("Larger string:", s1)
elif len2 > len1:
    print("Larger string:", s2)
else:
    print("Both strings are equal in length.")
